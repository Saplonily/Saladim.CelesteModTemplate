{
    "$schema": "http://json.schemastore.org/template",
    "author": "Saplonily",
    "classifications": [
        "Celeste",
        "Mod",
        "Everest"
    ],
    "identity": "Sap.CelesteModTemplate",
    "name": "Celeste Mod Template",
    "shortName": "sapcelestemod",
    "sourceName": "MyCelesteMod",
    "tags": {
        "language": "C#",
        "type": "project"
    },
    "symbols": {
        "celeste-root-path": {
            "type": "parameter",
            "datatype": "text",
            "replaces": "{CelesteRootPath_FromTemplate}",
            "defaultValue": "C:/Program Files (x86)/Steam/steamapps/common/Celeste"
        },
        "core-only": {
            "type": "parameter",
            "datatype": "bool",
            "replaces": "{CoreOnlyMod_FromTemplate}",
            "defaultValue": "false"
        }
    }
}